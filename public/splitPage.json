            {
              "type": "dynamicpage",
              "containerStyling": {
                "position": "relative"
              },
              "name": "left"
            }
          ]
        },
        {
          "type": "flexcontainer",
          "containerStyling": {
            "position": "relative",
            "border": "solid 1px green",
