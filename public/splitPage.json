            "margin": "5%"
          },
          "children": [
            {
              "type": "label",
              "containerStyling": {
                "position": "relative",
                "height": "30px",
                "margin": "10px"
              },
              "widgetStyling": {
                "fontWeight": "bold",
                "textAlign": "center"
              },
              "text": "/right/:json/:macros"
            },
            {
              "type": "dynamicpage",
              "containerStyling": {
                "position": "relative"
              },
              "name": "right"
